E,EAAE,CAAC,KAAK,GAAG,IAAI,CAAC,SAAS,EAAE,EAAE,CAAC,CAAC,CAAC;IAC3E,CAAC;CACJ;AA/DD,0BA+DC","sourcesContent":["import { Enser } from \"./Enser\";\nimport { Caja } from \"./Caja\";\n\n/**\n * Clase que representa la gestión de una mudanza, permitiendo manejar múltiples cajas de enseres.\n */\nexport class Mudanza<T extends Enser>{\n    /** Colección de cajas utilizadas en la mudanza. */\n    private cajas: Caja<T>[] = [];\n\n    /**\n     * Crea una instancia de gestión de mudanza.\n     */\n    constructor() {\n        this.cajas = [];\n    }\n\n    /**\n     * Agrega una caja a la colección de cajas de la mudanza.\n     * @param caja La caja a agregar.\n     */\n    public agregarCaja(caja: Caja<T>): void {\n        this.cajas.push(caja);\n    }\n\n    /**\n     * Elimina una caja de la colección basado en su índice.\n     * @param indice El índice de la caja a eliminar.\n     */\n    public eliminarCaja(indice: number): void {\n        if (indice < 0 || indice >= this.cajas.length) {\n            throw new Error(\"Índice de caja fuera de rango.\");\n        }\n        this.cajas.splice(indice, 1);\n    }\n\n    /**\n     * Obtiene la cantidad de cajas en la mudanza.\n     * @returns El número de cajas en la mudanza.\n     */\n    public cantidadCajas(): number {\n        return this.cajas.length;\n    }\n\n    /**\n     * Lista todas las cajas de la mudanza, incluyendo sus enseres.\n     * @returns Una cadena con la lista de cajas y sus enseres.\n     */\n    public listarCajas(): string {\n        if (this.cajas.length === 0) {\n            return \"No hay cajas.\";\n        }\n    \n        let resultado = \"\";\n        this.cajas.forEach((caja, i) => {\n            // Asumiendo que caja.listarEnseres() ahora devuelve un string.\n            resultado += `[Caja ${i + 1}]\\n${caja.listarEnseres()}\\n`;\n        });\n    \n        return resultado.trim(); // Elimina espacios en blanco extra al final\n    }\n\n    /**\n     * Calcula el peso total de la mudanza sumando el peso de todas las cajas.\n     * @returns El peso total de la mudanza.\n     */\n    public pesoTotalMudanza(): number {\n        return this.cajas.reduce((total, caja) => total + caja.pesoTotal(), 0);\n    }\n}"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"a029f4ab0008cf4478bafa399ee7a3173662a06d","contentHash":"393ff3fbc53e8b5172f2cdf61fe40720d1899c2692212632feffbb54b78b89f6"},"/home/usuario/PR6/ull-esit-inf-dsi-23-24-prct06-generics-solid-franmv21/src/ejercicio-2/FacturaSimple.ts":{"path":"/home/usuario/PR6/ull-esit-inf-dsi-23-24-prct06-generics-solid-franmv21/src/ejercicio-2/FacturaSimple.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":0},"end":{"line":3,"column":31}},"2":{"start":{"line":24,"column":8},"end":{"line":24,"column":21}},"3":{"start":{"line":25,"column":8},"end":{"line":25,"column":29}},"4":{"start":{"line":26,"column":8},"end":{"line":26,"column":33}},"5":{"start":{"line":27,"column":8},"end":{"line":27,"column":33}},"6":{"start":{"line":28,"column":8},"end":{"line":28,"column":49}},"7":{"start":{"line":29,"column":8},"end":{"line":29,"column":37}},"8":{"start":{"line":36,"column":8},"end":{"line":36,"column":31}},"9":{"start":{"line":42,"column":8},"end":{"line":42,"column":31}},"10":{"start":{"line":45,"column":0},"end":{"line":45,"column":38}}},"fnMap":{"0":{"name":"(anonymous_0)","decl":{"start":{"line":23,"column":4},"end":{"line":23,"column":5}},"loc":{"start":{"line":23,"column":86},"end":{"line":30,"column":5}},"line":23},"1":{"name":"(anonymous_1)","decl":{"start":{"line":35,"column":4},"end":{"line":35,"column":5}},"loc":{"start":{"line":35,"column":16},"end":{"line":37,"column":5}},"line":35},"2":{"name":"(anonymous_2)","decl":{"start":{"line":41,"column":4},"end":{"line":41,"column":5}},"loc":{"start":{"line":41,"column":19},"end":{"line":43,"column":5}},"line":41}},"branchMap":{"0":{"loc":{"start":{"line":23,"column":66},"end":{"line